@using MyQuery.Contracts.Skill;
@model List<SkillQueryViewModel>


<div class="foliox_tm_section">
    <div class="foliox_tm_skills">
        <div class="container">
            <div class="foliox_tm_main_title">
                <span>مهارت ها</span>
                <h3 > در اینجا میتوانید مهارت های من را مشاهده کنید</h3>
            </div>
            <div class="list">
                @foreach (var item in Model)
                {
                    <div class="foliox_tm_button_Hero">

                            <span> @item.Name </span>
                    </div>
                }
            </div>
        </div>
    </div>
</div>