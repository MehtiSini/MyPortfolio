<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="description" content="Name of your web site">
    <meta name="author" content="Marketify">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>Foliox - قالب شخصی</title>

    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;1,300;1,400;1,500;1,600;1,700;1,800&amp;display=swap" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/Theme/css/plugins.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/css/style.css" />
</head>
<body>

    <div id="preloader">
        <div class="loader_line"></div>
    </div>

    <div class="foliox_tm_all_wrap" data-magic-cursor="show">

        <div class="foliox_tm_mobile_menu">
            <div class="mobile_menu_inner">
                <div class="mobile_in">
                    <div class="logo">
                        <a href="#"><img src="~/Theme/img/logo/logo.png" alt="" /></a>
                    </div>
                    <div class="trigger">
                        <div class="hamburger hamburger--slider">
                            <div class="hamburger-box">
                                <div class="hamburger-inner"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="dropdown">
                <div class="dropdown_inner">
                    <ul class="anchor_nav">
                        <li class="current"><a href="#home">خانه</a></li>
                        <li><a href="#about">درباره من</a></li>
                        <li><a href="#portfolio">نمونه کار ها</a></li>
                        <li><a href="#service">خدمات</a></li>
                        <li><a href="#blog">وبلاگ</a></li>
                        <li><a href="#contact">ارتباط با من</a></li>
                    </ul>
                </div>
            </div>
        </div>

        <div class="foliox_tm_header">
            <div class="container">
                <div class="inner">
                    <div class="logo">
                        <a class="anchor" href="#home"><img src="~/Theme/img/logo/logo.png" alt="" /></a>
                    </div>
                    <div class="details">
                        <div class="menu">
                            <ul class="anchor_nav">
                                <li class="current"><a href="#home">خانه</a></li>
                                <li><a href="#about">درباره من</a></li>
                                <li><a href="#service">خدمات</a></li>
                                <li><a href="#portfolio">نمونه کار ها</a></li>
                                <li><a href="#blog">وبلاگ</a></li>
                                <li><a href="#contact">ارتباط با من</a></li>
                            </ul>
                        </div>
                        <div class="social">
                            <ul>
                                <li><a href="#"><i class="icon-facebook-1"></i></a></li>
                                <li><a href="#"><i class="icon-twitter-1"></i></a></li>
                                <li><a href="#"><i class="icon-behance-2"></i></a></li>
                                <li><a href="#"><i class="icon-instagram-1"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        @RenderBody()

        @await Component.InvokeAsync("Footer");

        <div class="mouse-cursor cursor-outer"></div>
        <div class="mouse-cursor cursor-inner"></div>

        <div class="progressbar">
            <a href="#"><span class="text">بالا</span></a>
            <span class="line"></span>
        </div>

    </div>

    <script data-cfasync="false" src="../../cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
    <script src="~/Theme/js/jquery.js"></script>
    <script src="~/Theme/js/plugins.js"></script>
    <script src="~/Theme/js/init.js"></script>

</body>

</html>